    <ctl name="Tx6 Mux Port" value="NONE"/>
    <ctl name="Tx7 Mux Port" value="NONE"/>

    <ctl name="Tx0 PortMux En" value="Off"/>
    <ctl name="Tx1 PortMux En" value="Off"/>
    <ctl name="Tx2 PortMux En" value="Off"/>
    <ctl name="Tx3 PortMux En" value="Off"/>
    <ctl name="Tx4 PortMux En" value="Off"/>
    <ctl name="Tx5 PortMux En" value="Off"/>
    <ctl name="Tx6 PortMux En" value="Off"/>
    <ctl name="Tx7 PortMux En" value="Off"/>

    <ctl name="Plgin0En" value="Off"/>
    <ctl name="Plgin1En" value="Off"/>
    <ctl name="Plgin2En" value="Off"/>
    <ctl name="Plgin3En" value="Off"/>
    <ctl name="Plgin4En" value="Off"/>
    <ctl name="Plgin5En" value="Off"/>
    <ctl name="Plgin6En" value="Off"/>
    <ctl name="Plgin7En" value="Off"/>
    <ctl name="Plgin8En" value="Off"/>
    <ctl name="Plgin9En" value="Off"/>

    <ctl name="Port ApllCLK Src" value="IAXXX_SRC_SYSCLK" />

    <path name="port-c">
    <!-- Port-C PDM4, DMIC1 -->
        <ctl name="Route Status" value="Active"/>
        <ctl name="PortC MicBias" value="1"/>
        <!--ctl name="PDM BCLK" value="IAXXX_PDM_CLK_3P_072MHZ"/-->
        <ctl name="Pdm CDC Setup" value="1"/>
        <ctl name="Pdm PortC Setup" value="1"/>
        <ctl name="Pdm PortC CDC DMic4 En" value="1"/>
        <ctl name="PDM Hos" value="0x10"/>
        <ctl name="PDM CDC0 Start" value="0x10"/>
        <ctl name="Rx0Chan GnRmp" value="STEP_2000"/>
        <ctl name="Rx0Ch EpGain" value="20"/>
        <ctl name="Rx0Chan Gain En" value="1"/>
        <ctl name="strm0 ASRC Mode" value="REDBOX_2-1"/>
        <ctl name="strm0 Master Strm Id" value="STREAMID_0"/>
        <ctl name="strm0 Format FrLn" value="16K_10MS"/>
        <ctl name="strm0 Format Sr" value="RATE_16K"/>
        <ctl name="strm0 Format Enc" value="ENCODING_AFLOAT"/>
        <ctl name="strm0 Dir" value="0"/>
        <ctl name="strm0 DC block en" value="1" />
        <ctl name="strm0 inter strm delay" value="0"/>
        <ctl name="strm0 Port" value="PDMI4"/>
        <ctl name="strm0 Port Enc" value="ENCODING_Q23"/>
        <ctl name="strm0 CH Mask En" value="1"/>
        <ctl name="Rx0 Mux Port" value="PDMI4"/>
        <ctl name="strm0 En" value="1"/>
    </path>

    <path name="port-b">
    <!-- Port-B PDM6, DMIC2 -->
        <ctl name="Route Status" value="Active"/>
        <ctl name="PortB MicBias" value="1"/>
        <!--ctl name="PDM BCLK" value="IAXXX_PDM_CLK_3P_072MHZ"/-->
        <ctl name="Pdm CDC Setup" value="1"/>
        <ctl name="Pdm PortB Setup" value="1"/>
        <ctl name="Pdm PortB CDC DMic6 En" value="1"/>
        <ctl name="PDM Hos" value="0x40"/>
        <ctl name="PDM CDC0 Start" value="0x40"/>
        <ctl name="Rx0Chan GnRmp" value="STEP_2000"/>
        <ctl name="Rx0Ch EpGain" value="20"/>
        <ctl name="Rx0Chan Gain En" value="1"/>
        <ctl name="strm0 ASRC Mode" value="REDBOX_2-1"/>
        <ctl name="strm0 Master Strm Id" value="STREAMID_0"/>
        <ctl name="strm0 Format FrLn" value="16K_10MS"/>
        <ctl name="strm0 Format Sr" value="RATE_16K"/>
        <ctl name="strm0 Format Enc" value="ENCODING_AFLOAT"/>
        <ctl name="strm0 Dir" value="0"/>
        <ctl name="strm0 DC block en" value="1" />
        <ctl name="strm0 inter strm delay" value="0"/>
        <ctl name="strm0 Port" value="PDMI6"/>
        <ctl name="strm0 Port Enc" value="ENCODING_Q23"/>
        <ctl name="strm0 CH Mask En" value="1"/>
        <ctl name="Rx0 Mux Port" value="PDMI6"/>
        <ctl name="strm0 En" value="1"/>
    </path>

    <path name="port-d">
    <!-- Port-D PDM3, Flicker -->
        <ctl name="Route Status" value="Active"/>
        <ctl name="CDC0 MicBias" value="1"/>
        <ctl name="PDM BCLK" value="IAXXX_PDM_CLK_3P_072MHZ"/>
        <ctl name="Pdm CDC Setup" value="1"/>
        <ctl name="Pdm CDC1 Setup" value="1"/>
        <ctl name="Pdm CDC1 CDC DMic3 En" value="1"/>
        <ctl name="PDM Hos" value="0x8"/>
        <ctl name="PDM CDC0 Start" value="0x8"/>
        <ctl name="Rx0Ch EpGain" value="20"/>
        <ctl name="Rx0Chan GnRmp" value="STEP_2000"/>
        <ctl name="Rx0Chan Gain En" value="1"/>
        <ctl name="strm0 ASRC Mode" value="REDBOX_2-1"/>
        <ctl name="strm0 Master Strm Id" value="STREAMID_0"/>
        <ctl name="strm0 Format FrLn" value="16K_10MS"/>
        <ctl name="strm0 Format Sr" value="RATE_16K"/>
        <ctl name="strm0 Format Enc" value="ENCODING_AFLOAT"/>
        <ctl name="strm0 Dir" value="0"/>
        <!-- ctl name="strm0 DC block en" value="1" /-->
        <ctl name="strm0 inter strm delay" value="0"/>
        <ctl name="strm0 Port" value="PDMI3"/>
        <ctl name="strm0 Port Enc" value="ENCODING_Q23"/>
        <ctl name="strm0 CH Mask En" value="1" />
        <ctl name="Rx0 Mux Port" value="PDMI3"/>
        <ctl name="strm0 En" value="1" />
    </path>

    <path name="port-cdc">
    <!-- Port-CDC PDM1, DMIC3 -->
        <ctl name="Route Status" value="Active"/>
        <ctl name="CDC0 MicBias" value="1"/>
        <!--ctl name="PDM BCLK" value="IAXXX_PDM_CLK_3P_072MHZ"/-->
        <ctl name="Pdm CDC Setup" value="1"/>
        <ctl name="Pdm CDC1 Setup" value="1"/>
        <ctl name="Pdm CDC0 CDC DMic1 En" value="1"/>
        <ctl name="PDM Hos" value="0x2"/>
        <ctl name="PDM CDC0 Start" value="0x2"/>
        <ctl name="Rx0Ch EpGain" value="20"/>
        <ctl name="Rx0Chan GnRmp" value="STEP_2000"/>
        <ctl name="Rx0Chan Gain En" value="1"/>
        <ctl name="strm0 ASRC Mode" value="REDBOX_2-1"/>
        <ctl name="strm0 Master Strm Id" value="STREAMID_0"/>
        <ctl name="strm0 Format FrLn" value="16K_10MS"/>
        <ctl name="strm0 Format Sr" value="RATE_16K"/>
        <ctl name="strm0 Format Enc" value="ENCODING_AFLOAT"/>
        <ctl name="strm0 Dir" value="0"/>
        <ctl name="strm0 DC block en" value="1" />
        <ctl name="strm0 inter strm delay" value="0"/>
        <ctl name="strm0 Port" value="PDMI1"/>
        <ctl name="strm0 Port Enc" value="ENCODING_Q23"/>
        <ctl name="strm0 CH Mask En" value="1"/>
        <ctl name="Rx0 Mux Port" value="PDMI1"/>
        <ctl name="strm0 En" value="1"/>
    </path>

    <path name="mic1-route">
    <!-- main mic -->
        <path name="port-c"/>
    </path>

    <path name="mic2-route">
    <!-- second mic -->
        <path name="port-b"/>
    </path>

    <path name="mic3-route">
    <!-- third mic -->
        <path name="port-cdc"/>
    </path>

    <path name="bargein-route">
    <!-- use aec mic -->
        <ctl name="Plgin4Ip Ep0 Conf" value="plugin7Out0"/>
        <ctl name="Plgin4Ip Ep1 Conf" value="plugin7Out1"/>
        <ctl name="Plgin4Ip Ep2 Conf" value="plugin6Out0"/>
        <ctl name="Plgin4En" value="Rx0Plgin4On"/>
        <ctl name="Plgin4Blk1En" value="1"/>
    </path>

    <path name="bargein-amp-ref">
    <!-- use aec mic -->
        <ctl name="PCM PortE Setup" value="1"/>
        <ctl name="PCM PortE Start" value="0x1FF"/>
        <ctl name="strm4 ASRC Mode" value="ASRC_ENABLE"/>
        <ctl name="strm4 Master Strm Id" value="STREAMID_0"/>
        <ctl name="strm4 Format FrLn" value="16K_10MS"/>
        <ctl name="strm4 Format Sr" value="RATE_16K" />
        <ctl name="strm4 Format Enc" value="ENCODING_AFLOAT"/>
        <ctl name="strm4 Dir" value="0"/>
        <!--ctl name="strm4 DC block en" value="1" /-->
        <ctl name="strm4 inter strm delay" value="0"/>
        <ctl name="strm4 Port" value="PCM4"/>
        <ctl name="strm4 Port Enc" value="ENCODING_Q15"/>
        <ctl name="strm4 CH Mask En" value="0xF0"/>
        <ctl name="strm4 En" value="1"/>
    </path>

    <path name="bargein-amp-ref-48k">
    <!-- use aec mic -->
        <ctl name="PCM PortE Setup" value="1"/>
        <ctl name="PCM PortE Start" value="0x1FF"/>
        <ctl name="strm4 ASRC Mode" value="ASRC_ENABLE"/>
        <ctl name="strm4 Master Strm Id" value="STREAMID_0"/>
        <ctl name="strm4 Format FrLn" value="48K_10MS"/>
        <ctl name="strm4 Format Sr" value="RATE_48K" />
        <ctl name="strm4 Format Enc" value="ENCODING_AFLOAT"/>
        <ctl name="strm4 Dir" value="0"/>
        <ctl name="strm4 inter strm delay" value="0"/>
        <ctl name="strm4 Port" value="PCM4"/>
        <ctl name="strm4 Port Enc" value="ENCODING_Q15"/>
        <ctl name="strm4 CH Mask En" value="0xF0"/>
        <ctl name="strm4 En" value="1"/>
    </path>

    <!-- Hotword detect with bargein, mic in HPM(S=-37dBFS)-->
    <path name="mic-route-external-clock">
        <ctl name="Port ApllCLK Src" value="IAXXX_EXT_OSC" />
        <ctl name="PDM Port AudCLK" value="IAXXX_AUD_PORT_32K" />
        <ctl name="PDM BCLK" value="IAXXX_PDM_CLK_3P_072MHZ"/>
        <path name="mic3-route" />
        <ctl name="Rx0Ch EpGain" value="0"/>
        <ctl name="Rx0Chan GnRmp" value="STEP_2000"/>
        <ctl name="Rx0Chan Gain En" value="1"/>
    </path>

    <!-- Normal hotword detect use, mic in LPM(S=-21dBFS)-->
    <path name="mic-route-internal-clock">
        <ctl name="Port ApllCLK Src" value="IAXXX_INT_OSC" />
        <ctl name="PDM Port AudCLK" value="IAXXX_AUD_PORT_32K" />
        <ctl name="PDM BCLK" value="IAXXX_PDM_CLK_0P_768MHZ"/>
        <path name="mic1-route" />
        <ctl name="Rx0Ch EpGain" value="1"/>
        <ctl name="Rx0Chan GnRmp" value="STEP_2000"/>
        <ctl name="Rx0Chan Gain En" value="1"/>
    </path>

    <path name="src-route-mic">
    <!-- bind src route on main-mic -->
        <ctl name="Plgin6Ip Ep0 Conf" value="RX0_ChanMgr"/>
        <ctl name="Plgin6En" value="Rx0Plgin6On"/>
        <ctl name="Plgin6Blk1En" value="1"/>
    </path>

    <path name="src-route-amp-ref">
    <!-- bind src route on main-mic -->
        <ctl name="Plgin7Ip Ep0 Conf" value="RX4_ChanMgr"/>
        <ctl name="Plgin7Ip Ep1 Conf" value="RX6_ChanMgr"/>
        <ctl name="Plgin7En" value="Rx0Plgin7On"/>
        <ctl name="Plgin7Blk1En" value="1"/>
    </path>

    <path name="hotword-route">
    <!-- enable hotword route -->
        <ctl name="Plgin0En" value="Rx0Plgin0On"/>
        <ctl name="Plgin0Blk1En" value="1"/>
    </path>

    <path name="hotword-route-with-bargein">
        <ctl name="Plgin0Ip Ep0 Conf" value="plugin4Out0"/>
        <path name="hotword-route"/>
    </path>

    <path name="hotword-route-without-bargein">
        <ctl name="Plgin0Ip Ep0 Conf" value="plugin6Out0"/>
        <path name="hotword-route"/>
    </path>

    <path name="ambient-route">
    <!-- enable ambient route -->
        <ctl name="Plgin2En" value="Rx0Plgin2On"/>
        <ctl name="Plgin2Blk1En" value="1"/>
    </path>

    <path name="ambient-route-with-bargein">
        <ctl name="Plgin2Ip Ep0 Conf" value="plugin6Out0"/>
        <path name="ambient-route"/>
    </path>

    <path name="ambient-route-without-bargein">
        <ctl name="Plgin2Ip Ep0 Conf" value="plugin6Out0"/>
        <path name="ambient-route"/>
    </path>

    <path name="chre-route">
    <!-- enable chre route -->
        <ctl name="Plgin5En" value="Rx0Plgin5On"/>
        <ctl name="Plgin5Blk1En" value="1"/>
    </path>

    <path name="chre-route-with-bargein">
        <ctl name="Plgin5Ip Ep0 Conf" value="plugin4Out0"/>
        <path name="chre-route"/>
    </path>

    <path name="chre-route-without-bargein">
        <ctl name="Plgin5Ip Ep0 Conf" value="plugin6Out0"/>
        <path name="chre-route"/>
    </path>

    <path name="oslo-route">
    <!-- enable oslo route -->
        <ctl name="sensor0 Ip Conf" value="plugin9Out1"/>
        <ctl name="sensor0 En" value="1"/>
        <ctl name="Plgin8Ip Ep0 Conf" value="SensorOut0"/>
        <ctl name="Plgin9Ip Ep0 Conf" value="plugin8Out0"/>
        <ctl name="Plgin8Blk1En" value="1"/>
        <ctl name="Plgin9Blk1En" value="1"/>
    </path>

    <path name="buffer-audio-route">
        <ctl name="Plgin1En" value="Rx0Plgin1On" />
        <ctl name="Plgin1Blk1En" value="1" />
    </path>

    <path name="buffer-route-with-bargein">
        <ctl name="Plgin1Ip Ep0 Conf" value="plugin4Out0" />
        <path name="buffer-audio-route" />
    </path>

    <path name="buffer-route-without-bargein">
        <ctl name="Plgin1Ip Ep0 Conf" value="plugin6Out0" />
        <path name="buffer-audio-route" />
    </path>

    <path name="music-buffer-route">
        <ctl name="Plgin3En" value="Rx0Plgin3On" />
        <ctl name="Plgin3Blk1En" value="1" />
    </path>

    <path name="music-buffer-with-bargein">
        <ctl name="Plgin3Ip Ep0 Conf" value="plugin6Out0" />
        <path name="music-buffer-route" />
    </path>

    <path name="music-buffer-without-bargein">
        <ctl name="Plgin3Ip Ep0 Conf" value="plugin6Out0" />
        <path name="music-buffer-route" />
    </path>

    <path name="music-audio-route">
        <path name="music-buffer-without-bargein" />
    </path>

    <!-- Ignore Downlink Audio route to temporary disable feature-->
    <path name="downlink-audio-route">
        <path name="music-buffer-without-bargein" />
        <!--ctl name="PCM PortA Setup" value="1" />
        <ctl name="PCM PortA Start" value="0x3F" />
        <ctl name="strm6 ASRC Mode" value="ASRC_ENABLE" />
        <ctl name="strm6 Master Strm Id" value="STREAMID_6" />
        <ctl name="strm6 Format FrLn" value="16K_10MS" />
        <ctl name="strm6 Format Sr" value="RATE_16K" />
        <ctl name="strm6 Format Enc" value="ENCODING_AFLOAT" />
        <ctl name="strm6 Dir" value="0" />
        <ctl name="strm6 inter strm delay" value="0" />
        <ctl name="strm6 Port" value="PCM0" />
        <ctl name="strm6 Port Enc" value="ENCODING_Q15" />
        <ctl name="strm6 CH Mask En" value="0x1000" />
        <ctl name="strm6 En" value="1" />
        <path name="music-buffer-with-bargein" /-->
    </path>
</mixer>
